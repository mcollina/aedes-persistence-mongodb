{
  "name": "aedes-persistence-mongodb",
  "version": "7.0.1",
  "description": "MongoDB persistence for Aedes ",
  "main": "persistence.js",
  "scripts": {
    "test": "standard && node test.js | faucet",
    "release": "read -p 'GITHUB_TOKEN: ' GITHUB_TOKEN && export GITHUB_TOKEN=$GITHUB_TOKEN && release-it --disable-metrics"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "git": {
      "tagName": "v${version}"
    },
    "hooks": {
      "before:init": [
        "npm run test"
      ]
    },
    "npm": {
      "publish": true
    }
  },
  "precommit": "test",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcollina/aedes-persistence-mongodb.git"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "keywords": [
    "Aedes",
    "MQTT",
    "broker",
    "mosca",
    "persistence",
    "mongodb"
  ],
  "author": "Matteo Collina <hello@matteocollina.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mcollina/aedes-persistence-mongodb/issues"
  },
  "homepage": "https://github.com/mcollina/aedes-persistence-mongodb#readme",
  "devDependencies": {
    "concat-stream": "^2.0.0",
    "faucet": "0.0.1",
    "mongo-clean": "^2.0.0",
    "mqemitter-mongodb": "^7.0.0",
    "pre-commit": "^1.2.2",
    "release-it": "^12.6.2",
    "standard": "^14.3.1",
    "tape": "^4.13.0"
  },
  "dependencies": {
    "aedes-cached-persistence": "^8.1.0",
    "escape-string-regexp": "^2.0.0",
    "mongodb": "^3.4.1",
    "native-url": "^0.2.4",
    "pump": "^3.0.0",
    "qlobber": "^5.0.0",
    "through2": "^3.0.0"
  }
}
